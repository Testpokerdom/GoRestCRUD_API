<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%d{HH:mm:ss}] [%level] %message %n" />
        </Console>
    </Appenders>

    <Loggers>

        <!-- Root Logger -->
        <Root level="info" additivity="true">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>


</Configuration>